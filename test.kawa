var int x;
var int y;
var bool a;
var bool b;

main {
    x = 5;
    y = 10;
    a = true;
    b = false;

    if (x + 3 > y - 2) {
        print(1);
    } else if (x * 2 == y) {
        print(2);
    } else if (x * 2 + 1 < y + 1) {
        print(3);
    }

    if (a && (x < y)) {
        if (x * 2 == 10) {
            print(4);
        } else if (!b && (y == 10)) {
            print(5);
        }
    }

    if ((x >= 0 && y <= 10) || (x == y && a)) {
        print(6);
    }

    if (!(x > y) && (a || b)) {
        print(7);
    }
}

