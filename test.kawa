var int x;
var bool b;

class point {
  attribute int x;
  attribute int y;
}

class colored_point {
  attribute int r;
  attribute int g;
  attribute int b;
  attribute point pos;
}

main {
  // Test object creation and field access
  pos = new point;
  pos.x = 10;
  pos.y = 20;
  print(pos.x);
  print(pos.y);
  
  // Test nested object creation and field access
  cp = new colored_point;
  cp.r = 255;
  cp.g = 128;
  cp.b = 0;
  cp.pos = new point;
  cp.pos.x = 100;
  cp.pos.y = 200;
  print(cp.r);
  print(cp.pos.y);
}



