var int x;
var int sum;
var bool flag;

main {
    x = 1;
    sum = 0;
    while (x <= 5) {
        sum = sum + x;
        x = x + 1;
    }
    print(sum);
    
    x = 1;
    sum = 0;
    while (x <= 3) {
        sum = sum + x * x;
        x = x + 1;
    }
    print(sum);
    
    x = 0;
    flag = true;
    while (flag) {
        x = x + 1;
        if (x >= 3) {
            flag = false;
        }
    }
    print(x);
}
